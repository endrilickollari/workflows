{
  "name": "workflows",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "bun run scripts/generate-schema.ts",
    "db:create-migration": "bun run scripts/create-pg-migration.ts",
    "db:migrate": "bun run scripts/drizzle-migrate.ts",
    "db:direct-setup": "bun run scripts/db-setup.ts",
    "db:setup": "bun run db:direct-setup",
    "db:reset": "rm -f data.db && bun run db:setup",
    "db:studio": "bunx drizzle-kit studio",
    "setup": "bun install && bun run db:setup"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "1.2.0",
    "@elysiajs/jwt": "1.2.0",
    "@elysiajs/swagger": "1.2.2",
    "better-auth": "^1.2.5",
    "better-sqlite3": "^11.9.1",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.0",
    "@neondatabase/serverless": "^0.6.0",
    "elysia": "1.2.25",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.2.5",
    "@types/better-sqlite3": "^7.6.5",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "bun-types": "latest",
    "drizzle-kit": "0.30.6",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
